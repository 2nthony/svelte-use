{
  "name": "@svelte-use/monorepo",
  "version": "0.11.0",
  "private": true,
  "description": "Collection of essential Svelte Utilities",
  "author": "2nthony <https://github.com/evillt>",
  "license": "MIT",
  "scripts": {
    "build": "esno scripts/build.ts",
    "build:redirects": "esno scripts/redirects.ts",
    "build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf dist types packages/*/dist",
    "docs": "vitepress dev packages",
    "docs:serve": "vitepress serve packages",
    "docs:build": "npm run update:full && vitepress build packages && npm run build:redirects && esno scripts/post-docs.ts",
    "publish:ci": "esno scripts/publish.ts",
    "release": "esno scripts/release.ts && git push --follow-tags",
    "update": "esno scripts/update.ts",
    "update:full": "esno scripts/update.ts && npm run build:types && npm run update:recent",
    "update:recent": "esno scripts/recently-updated.ts",
    "test": "echo TEST TODO"
  },
  "devDependencies": {
    "@2nthony/prettier-config": "1.0.0",
    "@iconify/json": "1.1.428",
    "@rollup/plugin-replace": "3.0.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.30",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.0.2",
    "@types/js-yaml": "4.0.4",
    "@types/node": "16.11.7",
    "@types/prettier": "2.4.2",
    "@vueuse/core": "6.9.1",
    "axios": "0.24.0",
    "bumpp": "7.1.1",
    "commitizen": "4.2.4",
    "consola": "2.15.3",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "dayjs": "1.10.7",
    "esbuild-register": "3.1.2",
    "esno": "0.9.1",
    "export-size": "0.4.0",
    "fast-glob": "3.2.7",
    "fs-extra": "10.0.0",
    "gray-matter": "4.0.3",
    "js-yaml": "4.1.0",
    "postcss": "8.3.11",
    "postcss-nested": "5.0.6",
    "prettier": "2.4.1",
    "prism-theme-vars": "0.2.2",
    "rimraf": "3.0.2",
    "rollup": "2.60.0",
    "rollup-plugin-dts": "4.0.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "rollup-plugin-uglify": "6.0.4",
    "simple-git": "3.5.0",
    "svelte": "3.44.1",
    "svelte-preprocess": "4.9.8",
    "tslib": "2.3.1",
    "typescript": "4.4.4",
    "unplugin-icons": "0.7.6",
    "unplugin-vue-components": "0.28.0",
    "vite": "2.6.14",
    "vite-plugin-windicss": "1.5.1",
    "vitepress": "0.17.3",
    "vue": "3.2.22",
    "vue-demi": "0.11.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": true
    }
  }
}
